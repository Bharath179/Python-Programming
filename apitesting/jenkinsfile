pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/Bharath179/Python-Programming.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Building project...'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
            }
        }
    }
}